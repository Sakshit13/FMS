@model FMS._23.V1.ViewModels.ItemViewModel

@{
    ViewData["Title"] = "Delete";
}


<h4 style="color: #FF00FF;text-align:center"><b>Are you sure you want to delete this part number?</b></h4>
<div>
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.PartNumber)
        </dt>
       
        <dd class="col-sm-10" style="margin-right: 10px;">
            
            @ViewBag.message
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="PartNumber" />
        <input type="submit" value="Delete" class="btn btn-danger" /> |
        <a asp-action="Index">Back to List</a>
    </form>
</div>
